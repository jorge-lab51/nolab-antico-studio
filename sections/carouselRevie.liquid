      assign list = product.metafields.revie.reviewProductList
      assign reviewCount = product.metafields.revie.reviewData.reviewCount
      assign stars = product.metafields.revie.reviewData.reviewAverageValue
    endif 
  endif 
  assign countTotal = 0
